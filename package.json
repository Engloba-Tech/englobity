{
	"name": "@engloba-tech/englobity",
	"version": "0.2.00",
	"description": "Customs web components with React and Material",
	"scripts": {
		"test": "cross-env NODE_ENV=test npm run lint && jest --coverage",
		"test:watch": "cross-env NODE_ENV=test jest --watch",
		"start": "webpack serve  --hot --open --mode development",
		"clean": "rimraf ./dist",
		"generate": "typedoc typings/index.d.ts",
		"build": "npm run clean && npm run build:umd && npm run build:es",
		"build:umd": "rollup -c",
		"build:es": "cross-env BABEL_ENV=es babel ./src --out-dir ./dist/es --ignore '**/*.test.js'",
		"prepublish": "npm run build",
		"lint": "eslint \"./src/**/*.{js,jsx}\""
	},
	"keywords": [
		"material-ui",
		"react",
		"components",
		"engloba",
		"validation",
		"chip",
		"input",
		"form"
	],
	"main": "dist/index.js",
	"module": "dist/es/index.js",
	"types": "dist/index.d.ts",
	"dependencies": {
		"@date-io/moment": "^1.3.13",
		"@emotion/react": "^11.11.4",
		"@emotion/styled": "^11.11.0",
		"@mui/material": "^5.15.12",
		"@mui/x-tree-view": "^6.17.0",
		"babel-polyfill": "^6.26.0",
		"material-ui-chip-input": "^2.0.0-beta.2",
		"prop-types": "^15.0.0",
		"react-dropzone": "^11.3.1",
		"react-material-ui-form-validator": "^2.1.2",
		"react-swipeable-views": "^0.14.0",
		"shortid": "^2.2.16"
	},
	"peerDependencies": {
		"@material-ui/core": "^1.0.0 || ^3.0.0 || ^4.0.0",
		"@material-ui/lab": "^4.0.0-alpha.56",
		"@material-ui/pickers": "^3.2.10",
		"react": "^15.0.0 || ^16.0.0 || ^17.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.11.6",
		"@babel/core": "^7.16.0",
		"@babel/plugin-external-helpers": "^7.10.4",
		"@babel/plugin-proposal-do-expressions": "^7.10.4",
		"@babel/plugin-proposal-export-default-from": "^7.10.4",
		"@babel/plugin-proposal-logical-assignment-operators": "^7.11.0",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
		"@babel/plugin-proposal-optional-chaining": "^7.11.0",
		"@babel/plugin-proposal-pipeline-operator": "^7.10.5",
		"@babel/plugin-transform-runtime": "^7.11.5",
		"@babel/preset-env": "^7.16.0",
		"@babel/preset-react": "^7.16.0",
		"@babel/register": "^7.11.5",
		"@material-ui/core": "^4.11.0",
		"@material-ui/icons": "^4.9.1",
		"@material-ui/lab": "^4.0.0-alpha.56",
		"@material-ui/pickers": "^3.2.10",
		"@testing-library/dom": "^7.24.1",
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.0.2",
		"@testing-library/react-hooks": "^3.4.1",
		"@typescript-eslint/eslint-plugin": "^4.0.0",
		"@typescript-eslint/parser": "^4.0.0",
		"@webpack-cli/serve": "^1.6.0",
		"@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
		"babel-eslint": "10.x",
		"babel-jest": "^26.3.0",
		"babel-loader": "^8.2.3",
		"babel-plugin-add-module-exports": "^1.0.2",
		"babel-plugin-dynamic-import-node": "^2.3.3",
		"cross-env": "^7.0.2",
		"css-loader": "^6.5.1",
		"enzyme": "^3.11.0",
		"eslint": "^7.5.0",
		"eslint-config-react-app": "^6.0.0",
		"eslint-plugin-flowtype": "^5.2.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jest": "^24.0.0",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-react": "^7.20.3",
		"eslint-plugin-react-hooks": "^4.0.8",
		"eslint-plugin-testing-library": "^3.9.0",
		"file-loader": "^6.2.0",
		"html-loader": "^1.3.2",
		"html-webpack-plugin": "^4.5.2",
		"jest": "^26.4.2",
		"moment": "^2.29.4",
		"poppins-font": "^1.0.1",
		"react": "^17.0.1",
		"react-dom": "^15.0.0 || ^16.0.0 || ^17.0.0",
		"react-hot-loader": "^4.13.0",
		"react-scripts": "^4.0.3",
		"react-test-renderer": "^17.0.1",
		"rimraf": "^3.0.2",
		"rollup": "^2.26.10",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-dts": "^2.0.1",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-uglify": "^5.0.2",
		"sass-loader": "^12.3.0",
		"source-map-loader": "^1.0.0",
		"style-loader": "^3.3.1",
		"typedoc": "^0.20.25",
		"typescript": "^4.1.3",
		"webpack": "^5.63.0",
		"webpack-cli": "^4.3.0",
		"webpack-dev-server": "^4.4.0"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"plugins": [
			"react-hooks"
		],
		"globals": {
			"fixture": true,
			"location": true
		},
		"rules": {
			"react-hooks/rules-of-hooks": "error",
			"react-hooks/exhaustive-deps": "warn",
			"no-throw-literal": "off",
			"react/jsx-fragments": "error",
			"react/prop-types": 1,
			"no-console": "warn",
			"no-debugger": "warn",
			"no-use-before-define": 2,
			"spaced-comment": "warn",
			"quotes": [
				"error",
				"single"
			],
			"semi": [
				"error",
				"always"
			]
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Engloba-Tech/englobity.git"
	},
	"author": "engloba-tech",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Engloba-Tech/englobity/issues"
	},
	"homepage": "https://github.com/Engloba-Tech/englobity#readme"
}
